{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "../logs/ServicesAPI.log",
          "rollingInterval": "Day",
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {CorrelationId} {Level:u3}] {Username} {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "dbConnection": "Host=localhost;Port=5432;Database=AppointmentsDb;Username=postgres;Password=1111",
    "sqlConnection": "server=.\\SQLEXPRESS; database=AppointmentsDb; Integrated Security=true"
  },
  "Routes": {
    "AuthorityRoute": "https://localhost:7141",
    "DoctorDTO": "https://localhost:7203/api/Doctors/collection",
    "PatientDTO": "https://localhost:7203/api/Patients/collection",
    "ServiceDTO": "https://localhost:7209/api/Services/collection"
  }
}
